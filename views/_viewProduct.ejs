<!--include header-->
<%- include('include/_header') %>
<!--include header-->
<!-- Main -->
<main id="site-main">
  <div class="container mt-5">
    <div class="container">
      <div class="justify-content-between d-flex">
        <div class="filter">
          <a class="btn btn-outline-primary" href="/"
            ><i class="fa-solid fa-backward"></i
          ></a>
        </div>
      </div>
    </div>
    <div class="form-title text-center">
      <h2 class="text-dark">Product Info</h2>
    </div>
    <!--product view-->
    <div class="row mt-5">
      <div class="col-md-12">
        <div class="card">
          <div class="card-body">
            <div class="row">
              <div class="col-md-6">
                <h5 class="card-title">Model Number</h5>
                <p class="card-text"><%= product.modelNumber %></p>
              </div>
              <div class="col-md-6">
                <h5 class="card-title">Product Type</h5>
                <p class="card-text"><%= product.productType %></p>
              </div>
            </div>
            <div class="row">
              <div class="col-md-12">
                <h5 class="card-title">Material</h5>
                <p class="card-text"><%= product.material %></p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!--product view-->
  </div>
</main>
<!-- Main -->
<!--include footer-->
<%- include('include/_footer') %>
<!--include footer-->
